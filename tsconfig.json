    "compilerOptions": {
        "module": "ES2020",
        "target": "ES2020",
        "allowJs": true,
        "checkJs": true,
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "forceConsistentCasingInFileNames": true,
        "noEmit": true
    },
    "exclude": [
        "node_modules"
    ]
}